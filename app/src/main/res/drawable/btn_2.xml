<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape android:shape="oval">
            <gradient
                android:type="linear"
                android:angle="225"
                android:startColor="#FF5B7F"
                android:endColor="#FC9970" />
